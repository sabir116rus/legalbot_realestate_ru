
# Переименуй в .env и заполни значения
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
OPENAI_API_KEY=your_openai_api_key_here

# Модель OpenAI (можешь заменить на нужную)
OPENAI_MODEL=gpt-4o-mini

# Количество документов из CSV для контекста
RAG_TOP_K=3

# Сообщение с политикой конфиденциальности, показывается в боте
PRIVACY_POLICY_MESSAGE=Откройте политику конфиденциальности, изучите условия и подтвердите согласие

# URL Telegram Web App с политикой конфиденциальности
PRIVACY_POLICY_WEBAPP_URL=https://sabir116rus.github.io/legalbot-policy/

# Интеграция с Google Drive (опционально)
GOOGLE_DRIVE_CREDENTIALS_FILE=/path/to/service-account.json
GOOGLE_DRIVE_LOGS_FOLDER_ID=your_logs_folder_id
GOOGLE_DRIVE_CONSULTATIONS_FOLDER_ID=your_consultations_folder_id
GOOGLE_DRIVE_REPORTS_FOLDER_ID=your_reports_folder_id

# Идентификаторы файлов Google Sheets для синхронизации (опционально)
GOOGLE_DRIVE_LOGS_FILE_ID=your_logs_file_id
GOOGLE_DRIVE_CONSULTATIONS_FILE_ID=your_consultations_file_id
GOOGLE_DRIVE_REPORTS_FILE_ID=your_reports_file_id
